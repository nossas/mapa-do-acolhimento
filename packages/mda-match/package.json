{
  "name": "mda-match",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "private": true,
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "~26.0.24",
    "@types/node": "~12.12.70",
    "@types/node-zendesk": "^1.4.0",
    "@types/throng": "^4.0.2",
    "@types/url-join": "^4.0.1",
    "@types/ws": "^7.4.7",
    "@types/yup": "^0.29.13",
    "faker": "^4.1.0",
    "jest": "26.0.1",
    "jest-extended": "^0.11.5",
    "rimraf": "~3.0.2",
    "subscriptions-transport-ws": "^0.9.19",
    "ts-jest": "^26.5.6"
  },
  "main": "dist/tsc/main.js",
  "types": "dist/tsc/main.d.ts",
  "browser": "dist/esbuild/browser.js",
  "bin": {
    "my-cli-tool": "dist/esbuild/cli.js"
  },
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build && yarn esbuild-node",
    "esbuild-browser": "esbuild src/browser.ts --bundle --minify --sourcemap=external --outfile=dist/esbuild/browser.js",
    "esbuild-browser:dev": "esbuild src/browser.ts --bundle --outfile=dist/esbuild/browser.js",
    "esbuild-browser:watch": "esbuild src/browser.ts --bundle --watch --outfile=dist/esbuild/browser.js",
    "esbuild-node": "esbuild src/cli.ts --bundle --platform=node --minify --sourcemap=external --outfile=dist/esbuild/cli.js",
    "esbuild-node:dev": "esbuild src/cli.ts --bundle --platform=node --sourcemap=external --outfile=dist/esbuild/cli.js",
    "esbuild-node:watch": "esbuild src/cli.ts --bundle --platform=node --watch --sourcemap=external --outfile=dist/esbuild/cli.js"
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.1.1",
    "@turf/turf": "5.1.6",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "axios": "^0.24.0",
    "colors": "^1.4.0",
    "cross-fetch": "^3.1.4",
    "debug": "^4.3.2",
    "dotenv-safe": "^8.2.0",
    "elastic-apm-node": "^3.21.1",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "node-zendesk": "1.4.0",
    "pino": "^6.11.3",
    "throng": "^4.0.0",
    "ws": "^7.5.5",
    "yup": "^0.29.3"
  },
  "peerDependencies": {
    "subscriptions-transport-ws": "^0.9.17"
  }
}
